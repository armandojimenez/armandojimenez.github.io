{
  "name": "mywebportfolio",
  "version": "1.2.2",
  "description": "",
  "main": "js/scripts.js",
  "scripts": {
    "watch": "npm run build:css && npm run build:js && echo 'Build complete. Use: npm run dev'",
    "dev": "npm run watch:css & npm run watch:js",
    "watch:css": "npx sass --watch scss/styles.scss css/styles.css --style compressed",
    "watch:js": "npx babel js/scripts.js --out-file js/scripts.min.js --presets=@babel/env --minified --watch",
    "build:css": "npx sass scss/styles.scss css/styles.css --style compressed",
    "build:js": "npx babel js/scripts.js --out-file js/scripts.min.js --presets=@babel/env --minified"
  },
  "keywords": [
    "portfolio",
    "personal",
    "website",
    "tech",
    "coding",
    "dev"
  ],
  "author": "Armando Jimenez",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-wait": "0.0.2"
  },
  "dependencies": {}
}
